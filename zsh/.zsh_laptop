# If you come from bash you might have to change your $PATH.
export PATH=$HOME/.local/bin:/opt/intel/bin:$PATH
source $HOME/.cargo/env

# Path to your oh-my-zsh installation.
export ZSH=/home/tom/.oh-my-zsh

ZSH_THEME="robbyrussell"

plugins=(git zsh-syntax-highlighting vi-mode history-substring-search)

source $ZSH/oh-my-zsh.sh

alias fuck='sudo $(fc -ln -1)'

alias vim='nvim'
alias paul='cd /home/tom/Documents/PaulFiles && paul -u tap'

# Import colorscheme from 'wal'
(wal -r &)

# Autojump
[[ -s /home/tom/.autojump/etc/profile.d/autojump.sh ]] && source /home/tom/.autojump/etc/profile.d/autojump.sh
